<form action="/Empwork/Add" method="post">
    <div class="form-group">
        <label for="dat1">Enter date</label>
        <input type="text" class="form-control" id="dat1" placeholder="Enter the date" name="date1">
    </div>
    <div class="form-group">
        <label for="work">Enter work description</label>
        <input type="text" class="form-control" id="work" placeholder="Work Description" name="des">
    </div>
    <div class="form-group">
        <label for="dura">Enter Duration</label>
        <input type="number" class="form-control" id="dura" placeholder="Duration" name="duration">
    </div>
    <div class="form-group">
        <label for="st">1:Completed 0:Uncompleted</label>
        <input type="number" class="form-control" id="st" placeholder="Status" name="status">

    </div>
    <br>
    <button type="submit" class="btn btn-success">Add</button>

</form>
